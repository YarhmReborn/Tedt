local Players1 = game:GetService("Players")
local LocalPlayer1 = Players1.LocalPlayer
local UIPath1
if LocalPlayer1.PlayerGui.MainGUI.Game:FindFirstChild("Inventory") ~= nil then
    UIPath1 = LocalPlayer1.PlayerGui.MainGUI.Game.Inventory.Main
else
    UIPath1 = LocalPlayer1.PlayerGui.MainGUI.Lobby.Screens.Inventory.Main
end
    local totalValue = 0
    for _, category in pairs(UIPath1.Weapons.Items.Container:GetChildren()) do
        for _, itemContainer in pairs(category.Container:GetChildren()) do
            if itemContainer.Name == "Christmas" or itemContainer.Name == "Halloween" then
                for _, item in pairs(itemContainer.Container:GetChildren()) do
                    if item:IsA("Frame") then
                        local itemName = item.ItemName.Label.Text
                        if values[itemName] then
                            local amount = item.Container.Amount.Text
                            if amount == "" or amount == "None" then
                                amount = 1
                            else
                                local num = tonumber(amount:match("x(%d+)"))
                                if num then
                                    amount = num
                                else
                                    amount = 1
                                end
                            end
                            totalValue = totalValue + values[itemName] * amount
                        end
                    end
                end
            else
                if itemContainer:IsA("Frame") then
                    local itemName = itemContainer.ItemName.Label.Text
                    if values[itemName] then
                        local amount = itemContainer.Container.Amount.Text
                        if amount == "" or amount == "None" then
                            amount = 1
                        else
                            local num = tonumber(amount:match("x(%d+)"))
                            if num then
                                amount = num
                            else
                                amount = 1
                            end
                        end
                        totalValue = totalValue + values[itemName] * amount
                    end
                end
            end
        end
    end
    for _, pet in pairs(UIPath1.Pets.Items.Container.Current.Container:GetChildren()) do
        if pet:IsA("Frame") then
            local itemName = pet.ItemName.Label.Text
            if values[itemName] then
                local amount = pet.Container.Amount.Text
                if amount == "" or amount == "None" then
                    amount = 1
                else
                    local num = tonumber(amount:match("x(%d+)"))
                    if num then
                        amount = num
                    else
                        amount = 1
                    end
                end
                totalValue = totalValue + values[itemName] * amount
            end
        end
    end
