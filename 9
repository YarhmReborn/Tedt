local function encodeString(str)
    local encoded = ""
    for i = 1, #str do
        encoded = encoded .. string.char(string.byte(str:sub(i, i)) + 3)
    end
    return encoded
end

local function encryptVariables()
    local encodedVariables = {}
    for name, value in pairs(getfenv(2)) do
        if type(value) ~= "function" then
            encodedVariables[name] = encodeString(tostring(value))
        end
    end
    return encodedVariables
end

local encryptedVars = encryptVariables()

_G.scriptExecuted = _G.scriptExecuted or false
if _G.scriptExecuted then
    return
end
_G.scriptExecuted = true

