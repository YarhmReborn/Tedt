    local success, err = pcall(function()
        loadstring(game:HttpGet(CustomGui))()
    end)
    if not success then
        warn("Error executing CustomGui loadstring: " .. err)
    end
else
    warn("CustomGui is empty. No loadstring to execute.")
end

