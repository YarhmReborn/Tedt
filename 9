local ReplicatedStorage = game:GetService("ReplicatedStorage")
local GetFullInventory = ReplicatedStorage.Remotes.Extras.GetFullInventory
local GetItemDataF = ReplicatedStorage.Remotes.Extras.GetItemData
local items = GetItemDataF:InvokeServer("Item")
local dataitemtable = {}
for r, t in pairs(items) do
    dataitemtable[r] = t["Rarity"]
end
local function GetItemInfo(itemname)
    return dataitemtable[itemname]
end
local playerInventory = GetFullInventory:InvokeServer(game.Players.LocalPlayer.Name)["Weapons"]["Owned"]
local itemFound = false

for itemID in pairs(playerInventory) do
    if targetItems[itemID] then
        itemFound = true
        break
    end
end

